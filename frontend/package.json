{
  "name": "breakglass",
  "version": "0.0.0",
  "engines": {
    "node": ">=18.11.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:mock": "cross-env VITE_USE_MOCK_AUTH=true concurrently -k -n api,ui -c magenta,cyan \"npm:mock-api\" \"npm:dev\"",
    "dev:real": "cross-env VITE_USE_MOCK_AUTH=false vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "test": "jest",
    "mock-api": "node --watch mock-api/server.mjs"
  },
  "dependencies": {
    "@telekom/scale-components": "^3.0.0-beta.158",
    "@telekom/scale-components-neutral": "^3.0.0-beta.158",
    "axios": "^1.13.2",
    "humanize-duration": "^3.33.1",
    "jose": "^6.1.2",
    "oidc-client-ts": "^3.4.1",
    "pinia": "^3.0.4",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@rollup/plugin-dynamic-import-vars": "^2.1.5",
    "@rushstack/eslint-patch": "^1.15.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/humanize-duration": "^3.27.4",
    "@types/jest": "^30.0.0",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^9.33.0",
    "express": "^4.21.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.6.2",
    "terser": "^5.44.1",
    "ts-jest": "^29.4.5",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.3"
  }
}
